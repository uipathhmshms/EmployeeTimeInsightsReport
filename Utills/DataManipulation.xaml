e clone&#xD;&#xA;            Dim newRow As DataRow = dtClone.NewRow()&#xD;&#xA;            newRow.ItemArray = CType(row.ItemArray.Clone(), Object()) ' Explicitly cast to Object()&#xD;&#xA;&#xD;&#xA;            ' Generate the key for duplicate check&#xD;&#xA;            Dim key As String = String.Join(&quot;|&quot;, checkColumns.Select(Function(col) row(col).ToString()))&#xD;&#xA;&#xD;&#xA;            If seenValues.Contains(key) Then&#xD;&#xA;                ' Replace duplicate values with empty string in the clone&#xD;&#xA;                For Each col In checkColumns&#xD;&#xA;                    newRow(col) = &quot;&quot;&#xD;&#xA;                Next&#xD;&#xA;            Else&#xD;&#xA;                seenValues.Add(key)&#xD;&#xA;            End If&#xD;&#xA;&#xD;&#xA;            ' Ensure &quot;מחלקה&quot; is only displayed for the first row of each department&#xD;&#xA;            If newRow(&quot;מחלקה&quot;).ToString() = prevDept Then&#xD;&#xA;                newRow(&quot;מחלקה&quot;) = &quot;&quot;&#xD;&#xA;            Else&#xD;&#xA;                prevDept = newRow(&quot;מחלקה&quot;).ToString()&#xD;&#xA;            End If&#xD;&#xA;&#xD;&#xA;            ' Ensure &quot;קוד סוכן&quot; is only displayed for the first row of each agent&#xD;&#xA;            If newRow(&quot;קוד סוכן&quot;).ToString() = prevAgentCode Then&#xD;&#xA;                newRow(&quot;קוד סוכן&quot;) = &quot;&quot;&#xD;&#xA;            Else&#xD;&#xA;                prevAgentCode = newRow(&quot;קוד סוכן&quot;).ToString()&#xD;&#xA;            End If&#xD;&#xA;&#xD;&#xA;            ' If this is a &quot;Total&quot; row, clear the &quot;קוד סוכן&quot; and &quot;מחלקה&quot; values&#xD;&#xA;            If newRow(&quot;שם סוכן&quot;).ToString().ToLower().Contains(&quot;total&quot;) Then&#xD;&#xA;                newRow(&quot;קוד סוכן&quot;) = &quot;&quot;&#xD;&#xA;                newRow(&quot;מחלקה&quot;) = &quot;&quot;&#xD;&#xA;            End If&#xD;&#xA;&#xD;&#xA;            ' Add the modified row to the